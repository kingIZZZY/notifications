{
    "name": "hypervel/notifications",
    "type": "library",
    "description": "The notifications package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "notifications",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Notifications\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "hyperf/config": "3.2.x-dev",
        "hyperf/stringable": "3.2.x-dev",
        "hyperf/contract": "3.2.x-dev",
        "hyperf/collection": "3.2.x-dev",
        "hyperf/context": "3.2.x-dev",
        "hyperf/database": "3.2.x-dev",
        "hyperf/di": "3.2.x-dev",
        "hypervel/broadcasting": "dev-edge-hyperf3.2",
        "hypervel/support": "dev-edge-hyperf3.2",
        "hypervel/mail": "dev-edge-hyperf3.2",
        "hypervel/queue": "dev-edge-hyperf3.2",
        "hypervel/object-pool": "dev-edge-hyperf3.2",
        "hypervel/translation": "dev-edge-hyperf3.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Notifications\\ConfigProvider"
        },
        "hypervel": {
            "providers": [
                "Hypervel\\Notifications\\NotificationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/broadcasting"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/support"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/mail"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/queue"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/object-pool"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/translation"
        }
    ]
}
